<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean class="com.xworkz.washmachine.WashingMachine">
		<constructor-arg name="brand" value="Godrej"></constructor-arg>
		<constructor-arg name="color" value="White"></constructor-arg>
	</bean>

	<bean class="java.lang.Integer">
		<constructor-arg value="1000"></constructor-arg>
	</bean>

	<bean class="java.lang.Byte">
		<constructor-arg value="80"></constructor-arg>
	</bean>

	<bean class="java.lang.Short">
		<constructor-arg value="20000"></constructor-arg>
	</bean>

	<bean class="java.lang.Long">
		<constructor-arg value="40000"></constructor-arg>
	</bean>

	<bean class="java.lang.Float">
		<constructor-arg value="400.8"></constructor-arg>
	</bean>

	<bean class="java.lang.Double">
		<constructor-arg value="2000.00"></constructor-arg>
	</bean>

	<bean class="java.lang.Character">
		<constructor-arg value="R"></constructor-arg>
	</bean>

	<bean class="java.lang.String">
		<constructor-arg value="Rakshitha"></constructor-arg>
	</bean>

	<bean class="java.lang.Boolean">
		<constructor-arg value="True"></constructor-arg>
	</bean>
</beans>